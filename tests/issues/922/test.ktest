EXPECT keyboard-report empty # Empty report on TD_0 release
RUN 4 ms
RELEASE TD_1
RUN 18 ms
EXPECT keyboard-report Key_B # TD_1 should time out, yielding `B`
RUN 1 cycle
EXPECT keyboard-report empty # Empty report after TD_1 timeout
RUN 11 ms

